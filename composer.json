{
    "name": "sinhang/elasticsearch",
    "description": "Elasticsearch client for hyperf",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "elasticsearch"
    ],
    "require": {
        "php": ">=8.0",
        "elasticsearch/elasticsearch": ">=8.7.1",
        "hyperf/guzzle": ">=2.2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Elasticsearch\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
